{{ form_start(form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.username) }}</div>
    <div class="col-md-4">{{ form_row(form.email) }}</div>
    <div class="col-md-4">{{ form_row(form.roles) }}</div>
    <div class="col-md-4">{{ form_row(form.enabled) }}</div>
    <div class="col-md-4">{{ form_row(form.first_name) }}</div>
    <div class="col-md-4">{{ form_row(form.last_name) }}</div>
    <div class="col-md-4">{{ form_row(form.birthday) }}</div>
    <div class="col-md-4">{{ form_row(form.sexe) }}</div>
    <button class="btn btn-primary" type="submit">{{ button| default('Cr√©er le profile') }}</button>
</div>
{{ form_end(form) }}